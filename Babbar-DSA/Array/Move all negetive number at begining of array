Input: 
1 -1 3 2 -7 -5 11 6
Output:
-5 -1 -7 2 3 1 11 6






class Solution {
  public:
    void segregateElements(vector<int>& arr) {
        // Your code goes here
        int n=arr.size()-1;
        int low=0,high=n;
        while(low<high){
            if(arr[low]<0){
                low++;
            }
            else if(arr[high]>0){
                high--;
            }
            else{
                swap(arr[low],arr[high]);
            }
        }
    }
};
