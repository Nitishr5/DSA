#include <iostream>
#include <unordered_map>
#include <vector>
using namespace std;

int main() {
    vector<int> arr = {1, 2, 2, 1, 3};

    unordered_map<int, int> freq;

    // Step 1: Count frequency
    for (int num : arr) {
        freq[num]++;
    }

    // Step 2: Print frequency
    for (auto it : freq) {
        cout << it.first << " -> " << it.second << endl;
    }

    return 0;
}






1 -> 2  
2 -> 2  
3 -> 1


