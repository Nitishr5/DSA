#include<iostream>
#include<unordered_set>
using namespace std;

int main()
{
    unordered_set<int>num;
    num.insert(1);
    num.insert(2);
    num.insert(3);
    num.insert(4);
    num.insert(5);
    
    for(int i:num)
    {
        cout<<i;
    }
}







//TAKE INPUT FROM USER====================+>


#include <iostream>
#include <unordered_set>
using namespace std;

int main()
{
    unordered_set<int> num;

    // 5 numbers input lena user se
    cout << "Enter 5 numbers:\n";
    for (int i = 0; i < 5; i++) {
        int x;
        cin >> x;
        num.insert(x);  // duplicates automatically ignore ho jaayenge
    }

    // Display the set elements
    cout << "Unique elements in the set:\n";
    for (int i : num)
    {
        cout << i << " ";
    }

    return 0;
}
