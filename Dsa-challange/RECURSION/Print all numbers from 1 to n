Print all numbers from 1 to n using recursion. 






#include <iostream>
using namespace std;

void print(int count, int n)
{
    //Base case
    
    if(count>n)
    {
        return;
    }
    
    cout<<count<<endl;
    print(count+1,n);
}
int main()
{
    //Print 1 to n
    int n=21;
    
    print(1,n);
    
}





