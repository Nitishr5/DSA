// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

void printDigits(int n){
    //base case
    if(n==0){
        return ;
    }
   
    int newValueofN=n/10;
    //baaki recursion sambhal lega
    printDigits(newValueofN); //other we can write printDigits(newValueofN);
    
     //1 case main solve karunga 
    int digit=n%10;
    cout<<digit<<" ";
    
}
int main() {
    
    int n=647;
if(n==0)    //This is for handel if user input 0 value
cout<<0<<endl;
    
    printDigits(n);
}
