#include <iostream>
using namespace std;
int fibo(int n)
{
    if(n==0)
    return 0;
    if(n==1)
    return 1;
    
    return fibo(n-1)+fibo(n-2);
}
int main() {
    // Write C++ code here
    int n=8;
   cout << "Fibonacci of " << n << " is " << fibo(n) << endl;  // Print the result
    return 0;
}
