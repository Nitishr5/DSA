
Getter =====> for fetch & read.
Setter =====> for set & update value.

#include <iostream>
using namespace std;

class Hero{
    //properties
    private:
    int health;
    
    public:
    char level;
    
    void print(){
        cout<<level<<endl;
    }
    
    int getHealth(){
        return health;
    }
    
    char getLevel(){
        return level;
    }
    
    void setHealth(int h){
        
        health=h;
      
    }
    
    void setLevel(char ch){
        level=ch;
    }
};
int main() {
    // Write C++ code here
   Hero ramesh;
   cout<<"Ramesh health is"<<ramesh.getHealth()<<endl;
   //use setter
   ramesh.setHealth(70);
   ramesh.level= 'A';
   cout<<"helth is: "<<ramesh.getHealth()<<endl;
   cout<<"Level is: "<<ramesh.level<<endl;
}
