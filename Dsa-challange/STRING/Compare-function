This is string pr-defined function compare()  
This return 0 if both string are same--->


#include <iostream>
using namespace std;

int main()
{
   string a="Love";
   string b="Love";
   
   if(a.compare(b)==0){
       cout<<"a and b are exactly same "<<endl;
   }
   else{
       cout<<"a and b are not same "<<endl;
   }
}












Make comapre() function user define ------>


int main()
{
   string a="Love";
   string b="Love";
   
   if(a.length()!=b.length())
    return false;
    
    for(int i=0;i<a.length();i++)
    {
        if(a[i]!=b[i]){
            return false;
        }
    }
    return true;
}
