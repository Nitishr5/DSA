// Online C++ compiler to run C++ program online
#include <iostream>
#include<stack>
using namespace std;
int main() {
    // Write C++ code here
 stack<int>s;
 
 //push operation
 s.push(2);
 s.push(3);
 
 //pop operation
  s.pop();
  
  if(s.empty())
  {
      cout<<"Stack is empty"<<endl;
  }
  else{
      cout<<"Stack is not-empty"<<endl;
  }
 
 cout<<"printing top element "<<s.top()<<endl;
 
 //size of stack
 cout<<"size of stack is "<<s.size()<<endl;
}
