
Example 1:

Input: nums = [1,1,2,3,3,4,4,8,8]
Output: 2
Example 2:

Input: nums = [3,3,7,7,10,11,11]
Output: 10




class Solution {
public:
    int singleNonDuplicate(vector<int>& nums) {
        int n=nums.size();
        if(n==1) return nums[0];
        int st=0,end=n-1;
        while(st<=end){
        int mid = (st+end) / 2;
            if(mid==0 && nums[0]!=nums[1]) return nums[mid]; // for first element
            if(mid==n-1 && nums[n-1]!=nums[n-2]) return nums[mid]; //for last element

            if(nums[mid-1]!=nums[mid]&&nums[mid]!=nums[mid+1])          return nums[mid];
            if(mid%2==0){ //even
                if(nums[mid-1]==nums[mid]){//left
                end=mid-1;
            } else{
                st=mid+1;
            }
        }
        else{
            if(nums[mid-1]==nums[mid]){//right
                st=mid+1;
            }
            else{
                end=mid-1;
            }
        }
    }
    
     return -1;

    }
};
