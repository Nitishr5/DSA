Example 1:

Input: nums = [1,2,3,4]
Output: [24,12,8,6]
Example 2:

Input: nums = [-1,1,0,-3,3]
Output: [0,0,9,0,0]






APPROCH 1----->>>  phle prefix product  &  sufix product nikal ke dono ka multiply kra denge ho jkaayega BUT,
                                                                                                              iska space-complaxity jyada ho jaayega===>    


class Solution {
public:
    vector<int> productExceptSelf(vector<int>& nums) {
        int n=nums.size();
        vector<int> ans(n,1);
        vector<int> prefix(n,1);
        vector<int>suffix(n,1);

        //prefix
        for(int i=1;i<n;i++){
            prefix[i]=prefix[i-1]*nums[i-1];
        }
        //sufix
        for(int i=n-2;i>=0;i--){
            suffix[i]=suffix[i+1]*nums[i+1];
        }
        //multiply prefix and sufix------
        for(int i=0;i<n;i++){
            ans[i]=prefix[i]*suffix[i];
        }





********** THIS APPROCH TAKE O(1) SPACE COMPLAXITY 

class Solution {
public:
    vector<int> productExceptSelf(vector<int>& nums) {
        int n=nums.size();
        vector<int> ans(n,1);
      

        //prefix => ans
        for(int i=1;i<n;i++){
            ans[i]=ans[i-1]*nums[i-1];
        }
        int suffix=1;
        //sufix
        for(int i=n-2;i>=0;i--){
            suffix*=nums[i+1];
            ans[i] *=suffix;
        }

        return ans;
    }
};
        return ans;
    }
};
