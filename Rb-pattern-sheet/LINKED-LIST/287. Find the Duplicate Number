Example 1:

Input: nums = [1,3,4,2,2]
Output: 2
Example 2:

Input: nums = [3,1,3,4,2]
Output: 3




class Solution {
public:
    int findDuplicate(vector<int>& a) {
       int slow = 0;
int  fast = 0;

while(true)
{
    slow = a[slow];
    fast = a[fast];
    fast = a[fast];

    if(slow == fast)
    {
        slow = 0;
        while(slow != fast)
        {
            slow = a[slow];
            fast = a[fast];
        }
        return slow;
    }
}

    }
};
