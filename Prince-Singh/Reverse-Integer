#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
    int num;
    cout<<"Enter a number";
    cin>>num;
    int rem,rev=0;
    
    
    while(num!=0){
        rem=num%10;
        rev=rev*10+rem;
        num=num/10;
    }
        cout<<rev;
    
}


1234====> 4321   reverse the number
